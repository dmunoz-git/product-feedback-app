{
    "name": "product-feedback-app",
    "version": "1.1.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --configuration development",
        "test:unit": "ng test",
        "test:coverage": "ng test --coverage",
        "test:e2e": "ng e2e",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "lint": "ng lint",
        "lint:fix": "ng lint --fix",
        "lint:styles": "stylelint \"**/*.scss\"",
        "lint:styles:fix": "stylelint --fix \"**/*.scss\"",
        "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
        "storybook": "npm run docs:json && start-storybook -p 6006",
        "build-storybook": "npm run docs:json && build-storybook"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.1.0",
        "@angular/cdk": "^13.1.1",
        "@angular/common": "~13.1.0",
        "@angular/compiler": "~13.1.0",
        "@angular/core": "~13.1.0",
        "@angular/forms": "~13.1.0",
        "@angular/platform-browser": "~13.1.0",
        "@angular/platform-browser-dynamic": "~13.1.0",
        "@angular/router": "~13.1.0",
        "@ngneat/transloco": "^3.1.1",
        "jest-preset-angular": "^11.0.1",
        "json-server": "^0.17.0",
        "rxjs": "~7.4.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/jest": "latest",
        "@angular-devkit/build-angular": "~13.1.2",
        "@angular-eslint/builder": "13.0.1",
        "@angular-eslint/eslint-plugin": "13.0.1",
        "@angular-eslint/eslint-plugin-template": "13.0.1",
        "@angular-eslint/schematics": "13.0.1",
        "@angular-eslint/template-parser": "13.0.1",
        "@angular/cli": "~13.1.2",
        "@angular/compiler-cli": "~13.1.0",
        "@babel/core": "^7.17.0",
        "@briebug/jest-schematic": "^4.0.0",
        "@commitlint/cli": "^16.2.4",
        "@commitlint/config-conventional": "^16.2.4",
        "@compodoc/compodoc": "^1.1.18",
        "@cypress/schematic": "^1.6.0",
        "@storybook/addon-actions": "^6.4.18",
        "@storybook/addon-essentials": "^6.4.18",
        "@storybook/addon-links": "^6.4.18",
        "@storybook/angular": "^6.4.18",
        "@storybook/builder-webpack5": "^6.4.18",
        "@storybook/manager-webpack5": "^6.4.18",
        "@testing-library/angular": "^11.0.4",
        "@testing-library/jest-dom": "^5.16.4",
        "@types/jest": "latest",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.3.0",
        "@typescript-eslint/parser": "5.3.0",
        "babel-loader": "^8.2.3",
        "cypress": "^9.5.4",
        "eslint": "^8.2.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^4.3.8",
        "jest": "latest",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "stylelint": "^14.2.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard-scss": "^3.0.0",
        "typescript": "4.4.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
